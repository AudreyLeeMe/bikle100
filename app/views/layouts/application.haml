!!! 5
%html
  %head
    %title http://bikle100.heroku.com
    %meta(http-equiv="content-type" content="text/html; charset=UTF-8")
    /
      %link(href="/stylesheets/layout.css" rel="stylesheet" type="text/css")
      %link(href="/stylesheets/content.css" rel="stylesheet" type="text/css")
      %link(href="/stylesheets/local.css" rel="stylesheet" type="text/css")
      %link(href="/stylesheets/application.css" rel="stylesheet" type="text/css")
    %link(href="/stylesheets/bikle.css" rel="stylesheet" type="text/css")

    %link(href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="stylesheet" type="text/css")
    %script(src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js" type="text/javascript")
    %script(src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.0/jquery-ui.min.js" type="text/javascript")
    %script(src="http://www.google.com/jsapi" type="text/javascript")
    %script(src="/javascripts/jquery.gvChart-1.0.min.js" type="text/javascript")
    %script(src="/javascripts/jquery.dataTables.min.js" type="text/javascript")
    %script(type="text/javascript")gvChartInit();
  %body
    #container
      #header
        %h1
          %span
            %a(href="http://bikle.com/")http://bikle.com
        %h2 contact: bikle@bikle.com
      #page
        #content
          =yield
        #sidebar
          .sidebar-node
            %h3 Dan
            Dan Bikle is an Oracle DBA and Rails Developer.
          .sidebar-node
            %h3 Links
            %a(href="/r10/site_map")Site Map
            %br/
            %a(href="/r10/about")About
            %br/
            %a(href="/r10/glossary")Glossary
            %br/
            %a(href="/r10/blog")Blog
            %br/
            %a(href="/tech")Tech
            %br/
            %a(href="/r10/forex")Forex
            %br/
            %a(href="/predictions")Predictions
            /
              %a(href="/r10/etfs")ETFs
              %br/
              %a(href="/r10/stocks")Stocks
              %br/
              %a(href="/r10/options")Options
              %br/
              %a(href="/r10/futures")Futures
              %br/
              %a(href="/r10/commodities")Commodities
              %br/
              %a(href="/r10/simulations")Simulations
              %br/
              %a(href="/r10/ib_api")IB API
          %hr/
          .sidebar-node
            %a(target="colorw" href="http://www.ficml.org/jemimap/style/color/wheel.html")
              %img(src="/images/hsvwheel.png" alt="4096 color wheel" height="63" width="142")
      #footer
        %a(href="http://bikle.com/")http://bikle.com
        is powered by 
        %a(href="http://api.rubyonrails.com/")Rails
        and styled with
        %a(href="http://quotedprintable.com/pages/scribbish")scribbish.
        %a(href="http://validator.w3.org/check?uri=referer" target="v")
          %img(src="http://www.w3.org/Icons/valid-xhtml10" alt="Valid XHTML 1.0 Strict" height="15" width="44")
        %a(href="http://jigsaw.w3.org/css-validator/" target="v2")
          %img(style="border:0;width:44px;height:15px" src="http://jigsaw.w3.org/css-validator/images/vcss-blue" alt="Valid CSS!")
